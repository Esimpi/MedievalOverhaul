<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="BaseFilth">
		<defName>DankPyon_Filth_Tar</defName>
		<label>tar puddle</label>
		<description>A puddle of flammable tar.</description>
		<thingClass>LiquidFuel</thingClass>
		<useHitPoints>true</useHitPoints>
		<tickerType>Normal</tickerType>
		<statBases>
			<Beauty>-10</Beauty>
			<Cleanliness>-15</Cleanliness>
			<MaxHitPoints>150</MaxHitPoints>
			<Flammability>2.0</Flammability>
		</statBases>
		<graphicData>
			<texPath>Filth/TarFilth</texPath>
		</graphicData>
		<filth>
			<ignoreFilthMultiplierStat>true</ignoreFilthMultiplierStat>
			<disappearsInDays>35~40</disappearsInDays>
			<rainWashes>true</rainWashes>
			<cleaningWorkToReduceThickness>70</cleaningWorkToReduceThickness>
			<canFilthAttach>true</canFilthAttach>
			<maxThickness>1</maxThickness>
			<cleaningSound>Interact_CleanFilth_Fluid</cleaningSound>
		</filth>
	</ThingDef>

	<ThingDef ParentName="BaseFilth">
		<defName>DankPyon_Filth_AcidBlood</defName>
		<label>scattered acid blood</label>
		<statBases>
			<Beauty>-30</Beauty>
			<Cleanliness>-5</Cleanliness>
		</statBases>
		<graphicData>
			<texPath>Things/Filth/Acid</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<drawSize>2.5</drawSize>
			<color>(1,1,1,0.5)</color>
		</graphicData>
		<filth>
			<ignoreFilthMultiplierStat>true</ignoreFilthMultiplierStat>
			<disappearsInDays>35~40</disappearsInDays>
			<rainWashes>true</rainWashes>
			<cleaningWorkToReduceThickness>70</cleaningWorkToReduceThickness>
			<canFilthAttach>true</canFilthAttach>
			<maxThickness>1</maxThickness>
			<cleaningSound>Interact_CleanFilth_Fluid</cleaningSound>
		</filth>
  </ThingDef>
  
  
</Defs>